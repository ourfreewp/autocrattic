{
    "$schema": "https://app.blockstudio.dev/schema",
    "apiVersion": 2,
    "name": "wp2/root-layout",
    "title": "Layout",
    "category": "wp2-theme",
    "icon": "layout",
    "description": "",
    "supports": {
        "className": false,
        "customClassName": false,
        "renaming": true,
        "reusable": false,
        "multiple": false,
        "lock": true,
        "html": false,
        "inserter": true
    },
    "attributes": {
        "className": {
            "type": "string",
            "default": "wp2-root"
        },
        "metadata": {
            "type": "object",
            "default": {
                "name": "Root Layout â€” Default"
            }
        },
        "templateLock": {
            "type": "string",
            "default": "all"
        },
        "lock": {
            "type": "object",
            "default": {
                "move": true,
                "remove": true
            }
        },
        "layout": {
            "type": "object",
            "default": {
                "type": "flex",
                "alignItems": "stretch",
                "flexWrap": "nowrap",
                "justifyContent": "stretch",
                "verticalAlignment": "space-between",
                "orientation": "vertical",
                "flexDirection": "column"
            }
        },
        "style": {
            "type": "object",
            "default": {
                "dimensions": {
                    "minHeight": "100vh"
                },
                "spacing": {
                    "padding": {
                        "top": "0",
                        "right": "0",
                        "bottom": "0",
                        "left": "0"
                    },
                    "margin": {
                        "top": "0",
                        "right": "0",
                        "bottom": "0",
                        "left": "0"
                    },
                    "blockGap": "0"
                }
            }
        }
    },
    "allowedBlocks": [
        "core/template-part"
    ],
    "blockstudio": {
        "attributes": [
            {
                "id": "option",
                "type": "select",
                "label": "Layout",
                "allowNull": "Select Layout",
